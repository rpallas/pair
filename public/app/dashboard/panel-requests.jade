panel.panel-default
    .panel-heading Requests
    .panel-body
        .row.request(ng-repeat="request in requests", ng-click="alert('test')")
            .col-lg-5
                a(href="/requests/{{ request._id }}")
                    img.profile-img-icon.img-responsive.img(ng-src="{{ request.otherUser.avatarUrl }}", alt="Profile picture icon")
                    | {{request.otherUser.displayName}}
            .col-lg-4 {{request.direction}}
            .col-lg-3 {{request.sentDateTime | date:'MMM d'}}